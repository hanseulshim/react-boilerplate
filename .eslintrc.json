{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "./webpack.config.js"
              }
        }
    },
    "env": {
        "browser": true,
        "node": true
    }
}